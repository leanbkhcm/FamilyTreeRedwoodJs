// api/db/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  body      String
  createdAt DateTime @default(now())
}


model Person {
  personId      Int      @id @default(autoincrement())
  fatherId      Int
  motherId      Int
  birthDay      DateTime 
}
